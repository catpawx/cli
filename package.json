{
  "name": "@catpawx/cli",
  "version": "1.0.5",
  "description": "🐾 猫爪联盟前端团队cli下载模板",
  "bin": {
    "create-catpawx": "dist/index.js",
    "cpx": "dist/index.js"
  },
  "keywords": [
    "cli",
    "template"
  ],
  "files": [
    "dist"
  ],
  "author": "相甫",
  "license": "ISC",
  "scripts": {
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "commit": "git-cz",
    "build": "npm run lint && npm run prettier && tsc --build",
    "lint": "eslint --fix src",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint-staged": "lint-staged --allow-empty"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@catpawx/commitlint-config-preset": "^0.1.0",
    "@catpawx/eslint-config-preset": "^2.0.0",
    "@catpawx/prettier-config-preset": "^2.0.0",
    "@commitlint/cli": "^19.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/minimist": "^1.2.5",
    "@types/prompts": "^2.4.9",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^8.12.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "semantic-release": "^24.2.5",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "download-git-repo": "^3.0.2",
    "minimist": "^1.2.8",
    "ora": "^3.2.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  }
}
