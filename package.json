{
  "name": "@catpawx/cli",
  "version": "0.0.2",
  "description": "💪🏻 猫爪联盟前端团队cli下载模板",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "catpawx": "src/index.js"
  },
  "keywords": [
    "cli",
    "template"
  ],
  "author": "相甫",
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "build": "rimraf dist && rollup -c",
    "lint": "eslint packages --fix --ext .js",
    "prettier": "prettier --write 'packages/**/*.{js,json,yml,yaml,css,scss,less,ts,tsx,md}'",
    "lint-staged": "lint-staged --allow-empty",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "download-git-repo": "^3.0.2",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.3.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@catpawx/eslint-config-preset": "^0.0.3",
    "@catpawx/prettier-config-preset": "^0.0.1",
    "@changesets/cli": "^2.27.6",
    "@commitlint/cli": "^19.3.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^8.12.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.17.2"
  }
}